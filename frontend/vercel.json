{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://princeshop-backend.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "regions": ["cdg1"]
}
